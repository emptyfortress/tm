<StackPanel ux:Class="todayText" Orientation="Horizontal" Margin="5" ItemSpacing="7" ContentAlignment="CenterLeft">
	<string ux:Property="Text"/>
	<string ux:Property="Quantity"/>
	<fa-caret-down Color="#fff" />
	<Text Value="{Property this.Quantity}" FontSize="25" Color="#000"  />
	<Text Value="{Property this.Text}" Color="#666" TextWrapping="Wrap"/>
</StackPanel>